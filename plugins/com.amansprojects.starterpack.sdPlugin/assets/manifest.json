		{
			"Platform": "mac",
			"MinimumVersion": "10.11"
		},
		{
			"Platform": "windows",
			"MinimumVersion": "10"
		},
		{
			"Platform": "linux"
		}
	],
	"Actions": [
		{
			"Icon": "icons/runCommand",
