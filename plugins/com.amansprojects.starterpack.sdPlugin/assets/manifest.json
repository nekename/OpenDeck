{
	"Name": "OpenDeck Starter Pack",
	"Description": "OpenDeck starter pack plugin",
	"Author": "Aman Khanna (nekename)",
	"Version": "2.9.1",
	"CodePaths": {
		"x86_64-pc-windows-msvc": "x86_64-pc-windows-msvc/bin/opendeck-starterpack.exe",
		"x86_64-apple-darwin": "x86_64-apple-darwin/bin/opendeck-starterpack",
		"aarch64-apple-darwin": "aarch64-apple-darwin/bin/opendeck-starterpack",
		"x86_64-unknown-linux-gnu": "x86_64-unknown-linux-gnu/bin/opendeck-starterpack",
		"aarch64-unknown-linux-gnu": "aarch64-unknown-linux-gnu/bin/opendeck-starterpack"
	},
	"CodePathWin": "x86_64-pc-windows-msvc/bin/opendeck-starterpack.exe",
